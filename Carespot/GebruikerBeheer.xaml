<Window x:Class="Carespot.GebruikerBeheer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Carespot"
        mc:Ignorable="d"
        Title="GebruikerBeheer" Height="800" Width="1000">
    <Grid>
        <ListView HorizontalAlignment="Left" Height="684" VerticalAlignment="Top" Width="972" Margin="10,10,0,0">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Naam" Width="550"/>
                    <GridViewColumn Header="Functie" Width="350"/>
                </GridView>
            </ListView.View>
        </ListView>
        <Image x:Name="imgGebruiker" HorizontalAlignment="Left" Height="41" Margin="14,705,0,0" VerticalAlignment="Top" Width="41" Source="image/trash.png" MouseLeftButtonUp="imgGebruiker_MouseLeftButtonUp" />
        <Image x:Name="imgSluitAf" HorizontalAlignment="Left" Height="41" Margin="941,705,0,0" VerticalAlignment="Top" Width="41" Source="image/cross-mark-on-a-black-circle-background.png" MouseLeftButtonUp="imgSluitAf_MouseLeftButtonUp"/>

    </Grid>
</Window>
